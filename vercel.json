{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/create-checkout-session",
      "destination": "/server.js"
    },
    {
      "source": "/api/verify-checkout",
      "destination": "/server.js"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "resetmypattern.com"
        }
      ],
      "destination": "https://mypatternreset.com/:path*",
      "permanent": true
    },
    {
      "source": "/letitout",
      "destination": "/letitout.html",
      "permanent": true
    },
    {
      "source": "/info",
      "destination": "/info.html",
      "permanent": true
    },
    {
      "source": "/theheartroom",
      "destination": "/theheartroom.html",
      "permanent": true
    },
    {
      "source": "/makers",
      "destination": "/makers.html",
      "permanent": true
    },
    {
      "source": "/legal",
      "destination": "/legal.html",
      "permanent": true
    },
    {
      "source": "/shop",
      "destination": "/shop/",
      "permanent": true
    },
    {
      "source": "/product/power-earrings",
      "destination": "/product/power-earrings.html",
      "permanent": true
    },
    {
      "source": "/product/balance-earrings",
      "destination": "/product/balance-earrings.html",
      "permanent": true
    },
    {
